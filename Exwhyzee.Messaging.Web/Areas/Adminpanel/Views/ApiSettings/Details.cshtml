@model Exwhyzee.Messaging.Web.Models.ApiSetting

@{
    ViewBag.Title = "Details";
}

<section class="content-header">
    <h1>
        Dashboard
        <small>Api Settings</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Api Settings</a></li>
        <li class="active"> Details</li>
    </ol>
</section>
<section class="content">

    <div class="box">
        <div class="box-header">
            <h3 class="box-title"> Api Setting Details </h3>

            <span class="pull-right">
                <a href="@Url.Content(" ../../apisettings")" class="btn btn-app"><i class="fa fa-backward"></i>Back</a>
                <a href="@Url.Content(" ../../apisettings/edit/"+ Model.ApiSettingId)" class="btn btn-app"><i class="fa fa-pencil-square-o"></i>Edit Api Setting</a>
            </span>
        </div>
        <div class="box-body">

            <div>
               
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.Name)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Name)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Sending)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Sending)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.CheckBalance)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.CheckBalance)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.IsDefault)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.IsDefault)
                    </dd>
                </dl>
            </div>
        </div>
    </div>
</section>