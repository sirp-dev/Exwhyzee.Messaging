@model IEnumerable<Exwhyzee.Messaging.Web.Models.MessageBatch>

@{
    ViewBag.Title = "Index";
}


<section class="content-header">
    <h1>
        Dashboard(Admin)
        <small>Submitted Sender Id</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Sender</a></li>
        <li class="active">ID</li>
    </ol>
</section>
<section class="content">

    <div class="box box-info">
        <div class="box-header">
            <h3 class="box-title">Sender Id </h3>

            
        </div>
        <div class="box-body table-responsive">
            <ul>
                @foreach (var item in Model)
                {
                <li>
                    @Html.DisplayFor(modelItem => item.MessageContent) | @item.DeliveredDate |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { @class = "btn btn-primary btn-xs" })

                </li>
                }
            </ul>
            </div>
        </div>
    </section>

